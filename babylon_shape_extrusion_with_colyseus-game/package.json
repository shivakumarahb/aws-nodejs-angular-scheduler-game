{
  "name": "babylon-game",
  "version": "0.0.0",
  "author": "stopnoanime",
  "homepage": "https://github.com/stopnoanime/babylon-game",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "backend-start": "cd backend && npm start",
    "build": "ng build",
    "deploy": "ng deploy --base-href=/babylon-game/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prettier": "npx prettier --write .",
    "prepare": "husky install",
    "e2e": "npm run backend-start & ng e2e",
    "cypress:run": "npm start & NODE_ENV=test npm run backend-start & cypress run"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/cdk": "^15.1.1",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/material": "^15.1.1",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "angular-notifier": "^12.0.0",
    "babylonjs": "^7.29.0",
    "babylonjs-gui": "^7.29.0",
    "colyseus.js": "^0.14.13",
    "earcut": "^3.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.2",
    "@angular/cli": "~15.1.2",
    "@angular/compiler-cli": "^15.1.0",
    "@cypress/schematic": "^2.5.0",
    "@types/earcut": "^2.1.4",
    "@types/jasmine": "~4.3.0",
    "angular-cli-ghpages": "^1.0.5",
    "autoprefixer": "^10.4.13",
    "cypress": "latest",
    "husky": "^8.0.3",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.21",
    "prettier": "2.8.3",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.9.4"
  }
}
